detach("package:tempoMonitoring", unload = TRUE)
detach("package:tempo", unload = TRUE)

install.packages("devtools")
library(devtools)

install_github("jarint/tempoMonitoring")
library(tempoMonitoring)

assemble()
generate()


install_github("jarint/tempo", force = TRUE)
library(tempo)

assemble()


